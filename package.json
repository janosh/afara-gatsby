{
  "private": true,
  "name": "afara-homepage",
  "title": "Afara",
  "description": "Homepage for German non-profit Afara built using Gatsby, styled-components and Contentful.",
  "homepage": "https://afara.foundation",
  "version": "1.0.0",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "repository": "https://github.com/janosh/afara",
  "dependencies": {
    "algoliasearch": "^4.3.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "2.24.3",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-algolia": "^0.11.1",
    "gatsby-plugin-catch-links": "2.3.11",
    "gatsby-plugin-google-analytics": "2.3.11",
    "gatsby-plugin-manifest": "2.4.18",
    "gatsby-plugin-offline": "3.2.18",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.19",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-remark-embed-video": "3.0.10",
    "gatsby-remark-images-contentful": "2.3.11",
    "gatsby-remark-responsive-iframe": "2.4.11",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-source-contentful": "2.3.26",
    "gatsby-transformer-remark": "2.8.25",
    "gatsby-transformer-sharp": "2.5.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.6.0",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.1",
    "styled-icons": "^10.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "contentful-management": "^6.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o -H 0.0.0.0",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build && gatsby serve -o -H 0.0.0.0"
  },
  "keywords": [
    "Afara",
    "non-profit",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 85,
    "arrowParens": "avoid"
  }
}
