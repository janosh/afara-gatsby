{
  "private": true,
  "name": "afara-homepage",
  "title": "Afara",
  "description": "Homepage for German non-profit Afara built using Gatsby, styled-components and Contentful.",
  "homepage": "https://afara.foundation",
  "version": "1.0.0",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "repository": "https://github.com/janosh/afara",
  "dependencies": {
    "algoliasearch": "^3.35.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "2.18.1",
    "gatsby-image": "2.2.33",
    "gatsby-plugin-algolia": "^0.5.0",
    "gatsby-plugin-catch-links": "2.1.17",
    "gatsby-plugin-google-analytics": "2.1.28",
    "gatsby-plugin-manifest": "2.2.29",
    "gatsby-plugin-offline": "3.0.22",
    "gatsby-plugin-react-helmet": "3.1.15",
    "gatsby-plugin-sharp": "2.3.2",
    "gatsby-plugin-styled-components": "3.1.13",
    "gatsby-remark-embed-video": "2.0.1",
    "gatsby-remark-images-contentful": "2.1.24",
    "gatsby-remark-responsive-iframe": "2.2.27",
    "gatsby-remark-smartypants": "2.1.16",
    "gatsby-source-contentful": "2.1.62",
    "gatsby-transformer-remark": "2.6.37",
    "gatsby-transformer-sharp": "2.3.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-instantsearch-dom": "^6.0.0",
    "styled-components": "^4.4.1",
    "styled-icons": "^8.6.0"
  },
  "devDependencies": {
    "contentful-management": "^5.11.3",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o -H 0.0.0.0",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build && gatsby serve -o -H 0.0.0.0"
  },
  "keywords": [
    "Afara",
    "non-profit",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ]
}
